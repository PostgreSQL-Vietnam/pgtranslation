#: pg_ctl.c:1912
#, c-format
msgid ""
"%s is a utility to initialize, start, stop, or control a PostgreSQL "
"server.\n"
"\n"
msgstr ""

#: pg_ctl.c:1913
#, c-format
msgid "Usage:\n"
msgstr ""

#: pg_ctl.c:1914
#, c-format
msgid "  %s init[db] [-D DATADIR] [-s] [-o OPTIONS]\n"
msgstr ""

#: pg_ctl.c:1915
#, c-format
msgid ""
"  %s start    [-D DATADIR] [-l FILENAME] [-W] [-t SECS] [-s]\n"
"                  [-o OPTIONS] [-p PATH] [-c]\n"
msgstr ""

#: pg_ctl.c:1917
#, c-format
msgid "  %s stop     [-D DATADIR] [-m SHUTDOWN-MODE] [-W] [-t SECS] [-s]\n"
msgstr ""

#: pg_ctl.c:1918
#, c-format
msgid ""
"  %s restart  [-D DATADIR] [-m SHUTDOWN-MODE] [-W] [-t SECS] [-s]\n"
"                  [-o OPTIONS] [-c]\n"
msgstr ""

#: pg_ctl.c:1920
#, c-format
msgid "  %s reload   [-D DATADIR] [-s]\n"
msgstr ""

#: pg_ctl.c:1921
#, c-format
msgid "  %s status   [-D DATADIR]\n"
msgstr ""

#: pg_ctl.c:1922
#, c-format
msgid "  %s promote  [-D DATADIR] [-W] [-t SECS] [-s]\n"
msgstr ""

#: pg_ctl.c:1923
#, c-format
msgid "  %s kill     SIGNALNAME PID\n"
msgstr ""

#: pg_ctl.c:1925
#, c-format
msgid ""
"  %s register [-D DATADIR] [-N SERVICENAME] [-U USERNAME] [-P PASSWORD]\n"
"                  [-S START-TYPE] [-e SOURCE] [-W] [-t SECS] [-s] [-o "
"OPTIONS]\n"
msgstr ""

#: pg_ctl.c:1927
#, c-format
msgid "  %s unregister [-N SERVICENAME]\n"
msgstr ""

#: pg_ctl.c:1930
#, c-format
msgid ""
"\n"
"Common options:\n"
msgstr ""

#: pg_ctl.c:1931
#, c-format
msgid "  -D, --pgdata=DATADIR   location of the database storage area\n"
msgstr ""

#: pg_ctl.c:1933
#, c-format
msgid ""
"  -e SOURCE              event source for logging when running as a "
"service\n"
msgstr ""

#: pg_ctl.c:1935
#, c-format
msgid ""
"  -s, --silent           only print errors, no informational messages\n"
msgstr ""

#: pg_ctl.c:1936
#, c-format
msgid "  -t, --timeout=SECS     seconds to wait when using -w option\n"
msgstr ""

#: pg_ctl.c:1937
#, c-format
msgid "  -V, --version          output version information, then exit\n"
msgstr ""

#: pg_ctl.c:1938
#, c-format
msgid "  -w, --wait             wait until operation completes (default)\n"
msgstr ""

#: pg_ctl.c:1939
#, c-format
msgid "  -W, --no-wait          do not wait until operation completes\n"
msgstr ""

#: pg_ctl.c:1940
#, c-format
msgid "  -?, --help             show this help, then exit\n"
msgstr ""

#: pg_ctl.c:1941
#, c-format
msgid ""
"If the -D option is omitted, the environment variable PGDATA is used.\n"
msgstr ""

#: pg_ctl.c:1943
#, c-format
msgid ""
"\n"
"Options for start or restart:\n"
msgstr ""

#: pg_ctl.c:1945
#, c-format
msgid "  -c, --core-files       allow postgres to produce core files\n"
msgstr ""

#: pg_ctl.c:1947
#, c-format
msgid "  -c, --core-files       not applicable on this platform\n"
msgstr ""

#: pg_ctl.c:1949
#, c-format
msgid "  -l, --log=FILENAME     write (or append) server log to FILENAME\n"
msgstr ""

#: pg_ctl.c:1950
#, c-format
msgid ""
"  -o, --options=OPTIONS  command line options to pass to postgres\n"
"                         (PostgreSQL server executable) or initdb\n"
msgstr ""

#: pg_ctl.c:1952
#, c-format
msgid "  -p PATH-TO-POSTGRES    normally not necessary\n"
msgstr ""

#: pg_ctl.c:1953
#, c-format
msgid ""
"\n"
"Options for stop or restart:\n"
msgstr ""

#: pg_ctl.c:1954
#, c-format
msgid ""
"  -m, --mode=MODE        MODE can be \"smart\", \"fast\", or \"immediate"
"\"\n"
msgstr ""

#: pg_ctl.c:1956
#, c-format
msgid ""
"\n"
"Shutdown modes are:\n"
msgstr ""

#: pg_ctl.c:1957
#, c-format
msgid "  smart       quit after all clients have disconnected\n"
msgstr ""

#: pg_ctl.c:1958
#, c-format
msgid "  fast        quit directly, with proper shutdown (default)\n"
msgstr ""

#: pg_ctl.c:1959
#, c-format
msgid ""
"  immediate   quit without complete shutdown; will lead to recovery on "
"restart\n"
msgstr ""

#: pg_ctl.c:1961
#, c-format
msgid ""
"\n"
"Allowed signal names for kill:\n"
msgstr ""

