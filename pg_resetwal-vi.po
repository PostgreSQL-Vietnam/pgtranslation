#: pg_resetwal.c:1312
#, c-format
msgid ""
"%s resets the PostgreSQL write-ahead log.\n"
"\n"
msgstr "%s đặt lại WAL cho PostgreSQL.\n"

#: pg_resetwal.c:1313
#, c-format
msgid ""
"Usage:\n"
"  %s [OPTION]... DATADIR\n"
"\n"
msgstr ""
"Cách sử dụng:\n"
"  %s [OPTION]... DATADIR\n"
"\n"

#: pg_resetwal.c:1314
#, c-format
msgid "Options:\n"
msgstr "Tùy chọn:\n"

#: pg_resetwal.c:1315
#, c-format
msgid ""
"  -c, --commit-timestamp-ids=XID,XID\n"
"                                 set oldest and newest transactions bearing\n"
"                                 commit timestamp (zero means no change)\n"
msgstr ""
"  -c, --commit-timestamp-ids=XID,XID\n"
"                                 thiết lập các giao dịch cũ nhất và mới nhất mang\n"
"                                 dấu thời gian commit (số không có nghĩa là không có thay đổi)\n"

#: pg_resetwal.c:1318
#, c-format
msgid " [-D, --pgdata=]DATADIR          data directory\n"
msgstr " [-D, --pgdata=]DATADIR          thư mục dữ liệu\n"

#: pg_resetwal.c:1319
#, c-format
msgid "  -e, --epoch=XIDEPOCH           set next transaction ID epoch\n"
msgstr "  -e, --epoch=XIDEPOCH           đặt epoch cho giao dịch ID tiếp theo\n"

#: pg_resetwal.c:1320
#, c-format
msgid "  -f, --force                    force update to be done\n"
msgstr "  -f, --force                    bắt buộc cập nhật để hoàn thành\n"

#: pg_resetwal.c:1321
#, c-format
msgid "  -l, --next-wal-file=WALFILE    set minimum starting location for new WAL\n"
msgstr "  -l, --next-wal-file=WALFILE    đặt vị trí bắt đầu tối thiểu cho WAL mới\n"

#: pg_resetwal.c:1322
#, c-format
msgid "  -m, --multixact-ids=MXID,MXID  set next and oldest multitransaction ID\n"
msgstr "  -m, --multixact-ids=MXID,MXID  đặt transaction ID đa nhiệm tiếp theo và cũ nhất\n"

#: pg_resetwal.c:1323
#, c-format
msgid "  -n, --dry-run                  no update, just show what would be done\n"
msgstr "  -n, --dry-run                  không cập nhật, chỉ hiện thị những thứ sẽ hoàn thành\n"

#: pg_resetwal.c:1324
#, c-format
msgid "  -o, --next-oid=OID             set next OID\n"
msgstr "  -o, --next-oid=OID             đặt OID tiếp theo\n"

#: pg_resetwal.c:1325
#, c-format
msgid "  -O, --multixact-offset=OFFSET  set next multitransaction offset\n"
msgstr "  -O, --multixact-offset=OFFSET  đặt offset cho transaction đa nhiệm tiếp theo\n"

#: pg_resetwal.c:1326
#, c-format
msgid "  -V, --version                  output version information, then exit\n"
msgstr "  -V, --version                  hiển thị thông tin phiên bản, sau đó kết thúc\n"

#: pg_resetwal.c:1327
#, c-format
msgid "  -x, --next-transaction-id=XID  set next transaction ID\n"
msgstr "  -x, --next-transaction-id=XID  đặt transaction ID tiếp theo\n"

